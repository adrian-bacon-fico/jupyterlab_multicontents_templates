{
  "ServerApp": {
    "jpserver_extensions": {
      "jupyterlab_multicontents_templates": true
    }
  },
  "JupyterLabMultiContentsTemplates": {
    "template_folders": [
      {
        "alias": "template-folde1",
        "manager_class": "IPython.html.services.contents.filemanager.FileContentsManager",
        "manager_kwargs": {"root_dir": "/tmp"}
      },
      {
        "alias": "template-folde2",
        "manager_class": "IPython.html.services.contents.filemanager.FileContentsManager",
        "manager_kwargs": {"root_dir": "/Users/lydian/"}
      }
    ]
  }
}
